{
    "keyboard_name": "Charybdis Nano (3x5 handwired) Splinky",
    "usb": {
        "device_version": "1.0.0"
    },
    "matrix_pins": {
        "cols": ["GP27", "GP28", "GP21", "GP6", "GP7"],
        "rows": ["GP29", "GP26", "GP5", "GP4"]
    },
    "diode_direction": "ROW2COL",
    "split": {
        "enabled": true,
        "transport": {
            "sync": {
                "matrix_state": true
            }
        }
    },
    "features": {
        "bootmagic": true,
        "command": false,
        "console": true,
        "extrakey": true,
        "mousekey": false,
        "nkro": false
    },
    "ws2812": {
        "pin": "GP0",
        "driver": "vendor"
    },
    "processor": "RP2040",
    "bootloader": "rp2040"
}
